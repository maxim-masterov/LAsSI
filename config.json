{
    "modules": [
        "2022", "foss/2022a"
    ],
    "batch_data": {
        "script_base_name": "test",
        "partition": "thin",
        "nodes": 1,
        "ntasks": 1,
        "time": 10,
        "has_gpus": false,
        "launcher": "srun",
        "executable_name": "dot.out",
        "executable_options": "1000000000 seq"
    },
    "test_setup": {
        "type": "omp",
        "recompile": true,
        "path_to_src": "examples",
        "compile_command": "g++",
        "compiler_flags": "-fopenmp -O3 -DUSE_OPENMP -o dot.out dot_test.cpp",
        "tasks_range": [1, 1],
        "thread_range": [1, 3],
        "//perf_regex": "Note that every backslash should sign e doubled",
        "perf_regex": "### Dot-product time:\\s+\\S+\\s+seconds"
    }
}
